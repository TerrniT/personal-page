---
import Link from '@components/Link.astro'

import {useI18n} from "@lib/i18n"

const {url} = Astro
const {p} = useI18n(url)
---


<Link href={p('/')}>
	<img
		src="/cat_logo.png"
		class="h-16 w-16 cursor-pointer sepia-0 transition-all duration-300 ease-in-out hover:scale-110 hover:sepia"
		alt="cat logo"
	/>
</Link>